<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="paperformat_a4_custom" model="report.paperformat">
        <field name="name">A4 Custom</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">297</field>  <!-- Altura personalizada em milímetros -->
        <field name="page_width">210</field>   <!-- Largura personalizada em milímetros -->
        <field name="orientation">Portrait</field>
        <field name="margin_top">10</field>    <!-- Margem superior em milímetros -->
        <field name="margin_bottom">10</field> <!-- Margem inferior em milímetros -->
        <field name="margin_left">10</field>   <!-- Margem esquerda em milímetros -->
        <field name="margin_right">10</field>  <!-- Margem direita em milímetros -->
        <field name="header_line" eval="False"/>
        <field name="header_spacing">10</field>
        <field name="dpi">90</field>
    </record>
</odoo>
